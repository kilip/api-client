{
  "name": "api-client",
  "version": "1.0.0",
  "description": "Library to integrate API Platform based project into Vue, React, Nuxt, Next",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "FORCE_COLOR=1 pnpm --filter './packages/**' prepack",
    "build:stub": "pnpm --filter './packages/**' prepack --stub",
    "cleanup": "rimraf 'packages/**/node_modules' 'node_modules'",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "JITI_ESM_RESOLVE=1 vitest run --dir packages"
  },
  "keywords": [],
  "author": "Anthonius Munthi",
  "license": "MIT",
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.13",
    "eslint": "^8.33.0",
    "eslint-plugin-jsdoc": "^39.7.4",
    "h3": "^1.1.0",
    "jiti": "^1.16.2",
    "lint-staged": "^13.1.0",
    "listhen": "^1.0.2",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "semver": "^7.3.8",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1",
    "vite": "^4.0.4",
    "vitest": "^0.28.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
