{
  "name": "@kilip/api-client-nuxt",
  "version": "0.1.0",
  "description": "Api Platform client module for Nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "module:build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run unit && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@kilip/api-client-pinia": "workspace:^0.1.0",
    "@nuxt/kit": "^3.1.1",
    "@pinia/nuxt": "^0.4.6",
    "pinia": "^2.0.29",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@kilip/api-client-core": "workspace:^0.1.0",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.1.1",
    "@nuxt/test-utils": "^3.1.1",
    "changelogen": "^0.4.1",
    "eslint": "^8.32.0",
    "nuxt": "^3.1.1",
    "vitest": "^0.28.2"
  }
}
